{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "frontend:dev": "npm run frontend:development",
        "frontend:development": "cross-env process.env.side=frontend NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "frontend:watch": "npm run frontend:development -- --watch",
        "frontend:watch-poll": "npm run frontend:watch -- --watch-poll",
        "frontend:hot": "cross-env process.env.side=frontend NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
        "frontend:prod": "npm run frontend:production",
        "frontend:production": "cross-env process.env.side=frontend NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "backend:dev": "npm run backend:development",
        "backend:development": "cross-env process.env.side=backend NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "backend:watch": "npm run backend:development -- --watch",
        "backend:watch-poll": "npm run backend:watch -- --watch-poll",
        "backend:hot": "cross-env process.env.side=backend NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "backend:prod": "npm run backend:production",
        "backend:production": "cross-env process.env.side=backend NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "alpinejs": "^2.7.3",
        "autoprefixer": "^10.0.4",
        "axios": "^0.19",
        "cross-env": "^7.0",
        "imagesloaded": "^4.1.4",
        "isotope-layout": "^3.0.6",
        "jquery": "^3.5.1",
        "jquery-ui": "^1.12.1",
        "laravel-mix": "^5.0.1",
        "line-awesome": "^1.3.0",
        "lodash": "^4.17.19",
        "moment": "^2.29.1",
        "nanogallery2-custom": "^1.0.0",
        "postcss": "^7.0.35",
        "postcss-css-variables": "^0.17.0",
        "postcss-import": "^12.0.1",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.30.0",
        "sass-loader": "^8.0.2",
        "swiper": "^6.3.5",
        "tailwindcss": "^1.8.0",
        "video.js": "^7.10.2",
        "videojs-plus": "^1.6.7",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {}
}
